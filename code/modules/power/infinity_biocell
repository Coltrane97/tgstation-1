/datum/design/biocell
	name = "Bio Power Cell"
	desc = "A selfrecharching power cell that holds 500 units of energy."
	id = "biocell"
	req_tech = list("powerstorage" = 1, "biotech"=6)
	build_type = PROTOLATHE
