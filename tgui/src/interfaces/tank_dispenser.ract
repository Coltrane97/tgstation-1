<ui-display>
    <ui-section label='Выдача'>
        <ui-button icon='{{data.plasma ? "square" : "square-o"}}' state='{{data.plasma ? null : "disabled"}}'
            action='plasma'>Плазма ({{Math.round(adata.plasma)}})</ui-button>
        <ui-button icon='{{data.oxygen ? "square" : "square-o"}}' state='{{data.oxygen ? null : "disabled"}}'
            action='oxygen'>Кислород ({{Math.round(adata.oxygen)}})</ui-button>
    </ui-section>
</ui-display>
